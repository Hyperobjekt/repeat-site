{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "commonjs": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "eslint-plugin-jsx-a11y"
  ],
  "rules": {
      "indent": [
          "off"
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "warn",
          "any"
      ],
      "react/prop-types": [
          "off"
      ]
  }
}
